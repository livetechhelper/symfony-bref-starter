{% extends 'base.html.twig' %}

{% block title %}Welcome to Symfony Bref Starter!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1>Hello there! âœ…</h1>

    <p>See your session info that comes from DynamoDB below (or filestorage if running local version)</p>

    {% if error is not empty %}

        <div>
            <p>There was an error trying to load your session...</p>
            <pre>
                {{ dump(error) }}
            </pre>
        </div>

    {% else %}

        <div>
            <ul>
                {{ dump(session) }}
            </ul>
        </div>

    {% endif %}





</div>
{% endblock %}
